

import stdlib.core.date



type User.ref = string
type User.t = {
    name : string
    password : string
    groups : list(Group.ref)
}


type Group.ref = string
type Group.t = {
    name : string
    users : list(User.ref)
}


type Facture.ref = int
type Facture.t = {
    date : Date.date
    montant : float
    emeteur : User.ref
    concerned : map(User.ref, float)
}
